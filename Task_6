// Create a new array where each element at index i is the sum of elements from 0 to i in the original array. 
// Sample input: arr = [1, 2, 3, 4]
// Sample output: [1, 3, 6, 10]
#include <iostream>
#include <vector>
using namespace std;

int main() {
    vector<int> arr = {1, 2, 3, 4};
    vector<int> runSum;
    int total = 0;

    for (int num : arr) {
        total += num;
        runSum.push_back(total);
    }

    for (int num : runSum) cout << num << " ";
    cout << endl;
    return 0;
}
