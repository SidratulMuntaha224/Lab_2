// Reverse the array
// Sample Input: arr = [1, 2, 3, 4]
// Sample Output: [4, 3, 2, 1]
#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
    vector<int> arr = {1, 2, 3, 4};
    reverse(arr.begin(), arr.end());

    for (int num : arr) cout << num << " ";
    cout << endl;
    return 0;
}
